# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5.2
# 비교 러너용: 콤마로 다중 모델 지정 (pnpm compare:llm)
OPENAI_MODELS=gpt-5-mini,gpt-4.1-mini

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-sonnet-4-6
# 비교 러너용
ANTHROPIC_MODELS=claude-sonnet-4-6,claude-haiku-4-5-20251001

# Google (Gemini)
GOOGLE_API_KEY=your_google_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here  # GOOGLE_API_KEY 대체 가능
GEMINI_MODEL=gemini-3-flash-preview
# 비교 러너용 (결제/쿼터 세팅 후 pro 추가 가능)
GEMINI_MODELS=gemini-2.5-flash,gemini-2.5-flash-lite

# DB (Supabase PostgreSQL — pooler 호스트는 Supabase 대시보드 Connect > ORMs 탭 참조)
DATABASE_URL="postgresql://postgres.[PROJECT_REF]:[PASSWORD]@aws-[N]-[REGION].pooler.supabase.com:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.[PROJECT_REF]:[PASSWORD]@aws-[N]-[REGION].pooler.supabase.com:5432/postgres"

# Supabase
SUPABASE_URL=https://[PROJECT_REF].supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Cron (Vercel Cron에서 자동 설정, 로컬용)
CRON_SECRET=your_cron_secret_here

# GA4
NEXT_PUBLIC_GA_ID=G-66VNNZXBLZ

# Client API URLs
NEXT_PUBLIC_API_URL=http://localhost:3001
EXPO_PUBLIC_API_URL=http://localhost:3001
